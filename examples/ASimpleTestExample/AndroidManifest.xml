<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="org.eurstein.test.androidexample"
          android:versionCode="6001129"
          android:versionName="6.0.0"
          android:installLocation="auto" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.eurstein.test.androidsimple.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="org.eurstein.test.androidsimple.sourceclass.TestParcelableActivity"></activity>
        <activity android:name="org.eurstein.test.androidsimple.sourceclass.TestIntentActivity1"></activity>
        <activity android:name="org.eurstein.test.androidsimple.sourceclass.TestIntentActivity2"></activity>
        <activity android:name="org.eurstein.test.androidsimple.sourceclass.TestPendingIntentActivity"></activity>
        <activity android:name="org.eurstein.test.androidsimple.sourceclass.LayoutPerformanceActivity"></activity>
    </application>

</manifest>
